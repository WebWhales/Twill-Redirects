{
  "name": "webwhales/twill-redirects",
  "description": "A twill package",
  "homepage": "https://github.com/WebWhales/Twill-Redirects",
  "license": "MIT",
  "require": {
    "php": ">=8",
    "area17/twill": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "TwillRedirects\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TwillRedirects\\TwillRedirectsServiceProvider"
      ]
    },
    "aliases": {
      "RedirectManager": "TwillRedirects\\Facades\\RedirectManager"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
